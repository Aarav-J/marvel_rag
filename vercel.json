{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "api/query.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/query",
      "dest": "/api/query.py"
    },
    {
      "src": "/api/?",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}